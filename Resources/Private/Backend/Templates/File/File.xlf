<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<xliff version="1.0">
	<file source-language="{sourceLanguage}" {f:if(condition: targetLanguage, then: 'target-language="{targetLanguage}"')} datatype="plaintext" original="messages" date="{f:format.date(format: 'c', date: 'now')}" product-name="translation_tools">
		<header>
			<generator>TranslationTools</generator>
		</header>
		<body>
			<f:for each="{translations}" as="translation">
			<trans-unit id="{translation.id}" xml:space="preserve">
				<source>{translation.source}</source>
				<target>{translation.target}</target>
			</trans-unit>
			</f:for>
		</body>
	</file>
</xliff>

